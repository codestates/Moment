{
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"extends": ["plugin:react/recommended"],
	"ignorePatterns": ["node_modules/"],
	"env": {
		"browser": true,
		"node": true
	},
	"globals": {
		"$": true
	},
	"rules": {
		"no-empty": "error",
		"default-case": "error",
		"eqeqeq": ["error", "always"],
		"prefer-template": "error"
	}
}
